class Solution {
public:
    int findMaxConsecutiveOnes(vector<int>& nums) {
        int sum=0,answer=0;
        for(int i=0;i<nums.size();i++)
        {
            if(nums[i]!=1)
            {
                answer=answer>sum?answer:sum;
                sum=0;
            }
            else
            {
                sum++;
            }
        }
        answer=answer>sum?answer:sum;
        return answer;
    }
};
