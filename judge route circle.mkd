bool judgeCircle(char* moves) {
    int x=0,y=0;
    int size=strlen(moves);
    for(int i=0;i<size;i++)
    {
        if(moves[i]=='U') y++;
        if(moves[i]=='D') y--;
        if(moves[i]=='R') x++;
        if(moves[i]=='L') x--;
    }
    if(x==0&&y==0) return true;
    else return false;
}
