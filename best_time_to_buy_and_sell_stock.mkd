int maxProfit(int* prices, int pricesSize) {
    int maxP=0;
    for(int i=0;i<pricesSize-1;i++)
    {
        for(int j=i+1;j<pricesSize;j++)
        {
            if((prices[j]-prices[i])>maxP) maxP=prices[j]-prices[i];
        }
    }
    return maxP;
}
